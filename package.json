{
  "name": "@tetherto/miningos-tpl-wrk-thing",
  "version": "0.0.1",
  "description": "MiningOS Template Worker Thing",
  "keywords": [
    "tether"
  ],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetherto/miningos-tpl-wrk-thing.git"
  },
  "engine": {
    "node": ">=20.0"
  },
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "NODE_ENV=test brittle tests/unit/*.js",
    "test:integration": "npm run test:integration:wrk && npm run test:integration:stats",
    "test:integration:wrk": "cd tests/integration && NODE_ENV=test brittle miner-wrk.js",
    "test:integration:stats": "cd tests/integration && NODE_ENV=test brittle miner-stats.js",
    "test:device": "NODE_ENV=test brittle tests/device.test.js",
    "test:coverage": "brittle --coverage tests/{,**/}*.test.js"
  },
  "dependencies": {
    "@bitfinex/lib-js-util-promise": "git+https://github.com/bitfinexcom/lib-js-util-promise.git",
    "async": "3.2.6",
    "bfx-facs-interval": "git+https://github.com/bitfinexcom/bfx-facs-interval.git",
    "bfx-facs-scheduler": "git+https://github.com/bitfinexcom/bfx-facs-scheduler.git",
    "debug": "4.4.1",
    "lib-js-util-base": "git+https://github.com/bitfinexcom/lib-js-util-base.git",
    "lib-js-util-promise": "git+https://github.com/bitfinexcom/lib-js-util-promise.git",
    "mingo": "6.5.6",
    "miningos-lib-stats": "git+https://github.com/tetherto/miningos-lib-stats.git",
    "svc-facs-miningos-thg-write-calls": "git+https://github.com/tetherto/svc-facs-miningos-thg-write-calls.git",
    "tether-wrk-base": "git+https://github.com/tetherto/tether-wrk-base.git",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "brittle": "3.16.3",
    "proxyquire": "2.1.3",
    "standard": "17.1.2",
    "tether-svc-test-helper": "git+https://github.com/tetherto/tether-svc-test-helper.git",
    "yargs": "17.7.2"
  }
}
